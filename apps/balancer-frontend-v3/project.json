{
  "name": "balancer-frontend-v3",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/balancer-frontend-v3",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project balancer-frontend-v3 --web",
  "targets": {
    "dev-bal": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run balancer-frontend-v3:dev-bal",
          "nx run balancer-frontend-v3:gen:graphql:watch"
        ],
        "parallel": true
      }
    },
    "gen-graphql-prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx run balancer-frontend-v3:gen:graphql:prod"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --project ./apps/balancer-frontend-v3/tsconfig.json --noEmit"
      }
    },
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "dependsOn": ["gen-graphql-prod"],
      "options": {
        "root": "apps/balancer-frontend-v3",
        "outputPath": "apps/balancer-frontend-v3/dist"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/balancer-frontend-v3"
        },
        "production": {}
      }
    }
  }
}
